    "@sidebase/nuxt-auth": "^0.8.2",
    "@types/node": "^20.11.30",
    "eslint": "^8.57.0",
    "oxlint": "^0.7.2",
    "playwright": "^1.46.1",
    "typescript": "^5.4.3",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.0.7"
  },
